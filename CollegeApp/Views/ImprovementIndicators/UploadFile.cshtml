@{
    ViewBag.Title = "تحميل ملف الشاهد";
}

<div class="col-md-12">
    <div class="card">
        @*<div class="card-header">
                <strong class="card-title">المستخدمون و الصلاحيات</strong>
            </div>*@
        <div class="card-body">
            <div style="text-align:center;">

                @using (Html.BeginForm("UploadFile", "ImprovementIndicators", null, FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary()
                   
                <div class="form-group">
                    <input type="file" id="dataFile" name="UploadFile" />
                    @Html.Hidden("ImprovementIndicatorsId",(int)ViewBag.ImprovementIndicatorsId )
                </div>

                    <div class="form-group">
                        <input type="submit" value="Upload" class="btn btn-default" />
                    </div>

                }

            </div>
        </div>
    </div>
</div>

